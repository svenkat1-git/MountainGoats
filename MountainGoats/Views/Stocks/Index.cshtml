@model IEnumerable<MountainGoatsBikes.Models.Stock>
    @{
    ViewData["Title"] = "Stocks";
    }

    <h2>Stocks</h2>
    <table class="table">
        <thead>
            <tr>
                <th>Store ID</th>
                <th>Product ID</th>
                <th>Quantity</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var stock in Model)
        {
            <tr>
                <td>@stock.StoreId</td>
                <td>@stock.ProductId</td>
                <td>@stock.Quantity</td>
            </tr>
        }
        </tbody>
    </table>
